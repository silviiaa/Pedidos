<form method="post" action="{{ path('restaurante_delete', {'codRes': restaurante.codRes}) }}" onsubmit="return confirm('¿Estás seguro de borrar este restaurante?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ restaurante.codRes) }}">
    <button class="btn">Borrar</button>
</form>
